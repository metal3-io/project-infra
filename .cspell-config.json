{
  "version": "0.2",
  "language": "en",
  "ignorePaths": ["**/.github/workflows/**"],
  "patterns": [
    {
      "name": "multiline-code-block",
      "pattern": "/^(>)?\\s*```(?:.|\\s)+?^(>)?\\s*```/mig"
    },
    {
      "name": "inline-code",
      "pattern": "/`[^`].*`[^`]/g"
    }
  ],
  "ignoreRegExpList": ["multiline-code-block", "inline-code"],
  "words": [
    "autoscaler",
    "baremetal",
    "capi",
    "capm",
    "checkconfig",
    "cherrypick",
    "cherrypicking",
    "cicd",
    "clusterapi",
    "clusterctl",
    "devuser",
    "diskimage",
    "estjorvas",
    "gerrit",
    "ghprb",
    "HMAC",
    "ipam",
    "jsonnet",
    "keypair",
    "kubeadm",
    "kubeadmcontrolplane",
    "kubeconfig",
    "kubelet",
    "kustomization",
    "kustomizations",
    "loadbalancer",
    "machinedeployment",
    "nordix",
    "openstackmachinetemplate",
    "presubmits",
    "tmpfs",
    "xerces"
  ]
}
